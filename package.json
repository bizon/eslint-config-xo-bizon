{
  "name": "eslint-config-xo-bizon",
  "version": "2.0.0",
  "description": "ESLint shareable config to be used with xo or eslint-config-xo",
  "main": "index.cjs",
  "repository": "git@github.com:bizon/eslint-config-xo-bizon.git",
  "author": "Bertrand Marron",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "index.cjs"
  ],
  "keywords": [
    "eslintconfig",
    "xo",
    "xoxo",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint",
    "bizon"
  ],
  "dependencies": {},
  "devDependencies": {
    "@bizon/semantic-release-config": "^2.1.0",
    "semantic-release": "^23.0.2",
    "xo": "^0.56.0"
  },
  "xo": {
    "extends": ".",
    "semicolon": false,
    "space": 2,
    "prettier": true
  },
  "prettier": {
    "printWidth": 100
  },
  "release": {
    "extends": "@bizon/semantic-release-config"
  }
}
